{
  "manifest_version": 3,
  "name": "YouTube Speed Booster",
  "version": "1.0",
  "description": "Add 2x, 3x, and 4x speed buttons to YouTube player.",
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*", "*://youtube.com/*"],
      "js": ["content.js"],
      "css": ["style.css"],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": ["activeTab"],
  "icons": {
    "48": "icon.png"
  }
}
